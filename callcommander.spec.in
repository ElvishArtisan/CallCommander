cp mlhpi/.libs/libmlhpi.a $RPM_BUILD_ROOT@LOCAL_PREFIX@/@CALL_LIB_PATH@/
cp mlhpi/.libs/libmlhpi.la $RPM_BUILD_ROOT@LOCAL_PREFIX@/@CALL_LIB_PATH@/
